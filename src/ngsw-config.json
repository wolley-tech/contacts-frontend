{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": { "files": ["/*.html", "/*.css", "/manifest.webmanifest"] }
    }
  ],
  "dataGroups": [
    {
      "name": "api",
      "urls": ["http://localhost:8080/api/**"],
      "cacheConfig": { "maxSize": 50, "maxAge": "1h", "strategy": "freshness" }
    }
  ]
}
